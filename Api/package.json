{
  "name": "IOM",
  "version": "3.3.6",
  "description": "IOM - Api",
  "main": "src/App.ts",
  "scripts": {
    "migrate": "node_modules/.bin/knex --knexfile ./repos/knexfile.js migrate:latest",
    "seed": "node_modules/.bin/knex --knexfile ./repos/knexfile.js seed:run",
    "build": "npm run compile-addon && npm run exec-builder && npm run compile-api",
    "compile-api": "tsc --build && tsc-alias && echo \"\\033[0;34m➤ API compile \\033[0m \"",
    "watch-compile-api": "tsc --build --watch | tsc-alias -w | echo \"\\033[0;34m➤ API compile watched \\033[0m \"",
    "watch-start": "node --watch prod/App.js",
    "start": "node prod/App.js",
    "test": "node_modules/.bin/jest -c jest.config.json --coverage",
    "test:watch": "node_modules/.bin/jest --watch -c jest.config.json",
    "lint": "eslint . --ext .ts",
    "fix-lint": "eslint --fix . --ext .ts"
  },
  "keywords": [],
  "author": "Ruby",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.3.0",
    "express-validator": "^6.14.2",
    "fast-xml-parser": "^4.0.12",
    "helmet": "^5.0.2",
    "knex": "^1.0.7",
    "knex-migrate-sql-file": "^1.0.3",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.3",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tsc-alias": "^1.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bindings": "^1.5.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/jest": "^29.2.3",
    "@types/nodemailer": "^6.4.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "8.15.0",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
